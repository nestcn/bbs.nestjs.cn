{
  "version": 1,
  "projects": {
    "libs": {
      "root": "libs",
      "sourceRoot": "libs",
      "projectType": "library",
      "prefix": "myworkspace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "testing/jest.libs.config.js",
            "tsConfig": "testing/tsconfig.libs.spec.json",
            "passWithNoTests": true,
            "setupFile": "testing/test-setup.ts"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "testing/tsconfig.libs.json",
              "testing/tsconfig.libs.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "xplat": {
      "root": "xplat",
      "sourceRoot": "xplat",
      "projectType": "library",
      "prefix": "myworkspace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "testing/jest.xplat.config.js",
            "tsConfig": "testing/tsconfig.xplat.spec.json",
            "passWithNoTests": true,
            "setupFile": "testing/test-setup.ts"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "testing/tsconfig.xplat.json",
              "testing/tsconfig.xplat.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "zlt-web-zlt": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/zlt/web-zlt",
      "sourceRoot": "apps/zlt/web-zlt/src",
      "prefix": "myworkspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/zlt/web-zlt",
            "index": "apps/zlt/web-zlt/src/index.html",
            "main": "apps/zlt/web-zlt/src/main.ts",
            "polyfills": "apps/zlt/web-zlt/src/polyfills.ts",
            "tsConfig": "apps/zlt/web-zlt/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/zlt/web-zlt/src/favicon.ico",
              "apps/zlt/web-zlt/src/assets"
            ],
            "styles": [
              "apps/zlt/web-zlt/src/styles.scss",
              "apps/zlt/web-zlt/src/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/zlt/web-zlt/src/environments/environment.ts",
                  "with": "apps/zlt/web-zlt/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "zlt-web-zlt:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "zlt-web-zlt:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "zlt-web-zlt:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/zlt/web-zlt/tsconfig.app.json",
              "apps/zlt/web-zlt/tsconfig.spec.json",
              "apps/zlt/web-zlt/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/zlt/web-zlt/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/zlt/web-zlt/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zlt-web-zlt-e2e": {
      "root": "apps/zlt/web-zlt-e2e",
      "sourceRoot": "apps/zlt/web-zlt-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/zlt/web-zlt-e2e/cypress.json",
            "tsConfig": "apps/zlt/web-zlt-e2e/tsconfig.e2e.json",
            "devServerTarget": "zlt-web-zlt:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "zlt-web-zlt:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/zlt/web-zlt-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/zlt/web-zlt-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nstudio/xplat"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
